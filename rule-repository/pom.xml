<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.kuali.student.brms</groupId>
	<artifactId>rule-repository</artifactId>
	<packaging>jar</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>Kuali Student BRMS - rule-repository</name>
	<description>Rules repository.</description>
	<parent>
		<groupId>org.kuali.student.brms</groupId>
		<artifactId>brms-ri</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>

	<build>
		<finalName>rule-repository</finalName>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-clean-plugin</artifactId>
					<configuration>
						<filesets>
							<fileset>
								<directory>./</directory>
								<includes>
									<include>repository**/*</include>
									<include>repository*.xml</include>
									<include>repository</include>
								</includes>
								<followSymlinks>false</followSymlinks>
							</fileset>
						</filesets>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<dependencies>
        <dependency>
            <groupId>org.kuali.student.brms</groupId>
            <artifactId>rules-common</artifactId>
            <version>0.0.1-SNAPSHOT</version>
