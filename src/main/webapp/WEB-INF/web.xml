    <url-pattern>/org.kuali.student.ui.personidentity.PersonIdentity/PersonIdentityService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>org.kuali.student.ui.personidentity.server.gwt.LearningUnitAppServiceImplGwt/org.kuali.student.ui.personidentity.PersonIdentity/LearningUnitAppService</servlet-name>
    <url-pattern>/org.kuali.student.ui.personidentity.PersonIdentity/LearningUnitAppService</url-pattern>
  </servlet-mapping>


	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

</web-app>
