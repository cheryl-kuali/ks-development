<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">

	<users xmlns="ns:workflow/User" xsi:schemaLocation="ns:workflow/User resource:User">
		<user>
			<workflowId>csubmit</workflowId>
			<emplId>submitter1</emplId>
			<authenticationId>csubmit</authenticationId>
			<uuId>csubmit</uuId>
			<emailAddress>csubmit@umd.edu</emailAddress>
			<displayName>Course Submitter</displayName>
			<givenName>Course</givenName>
			<lastName>Submitter</lastName>
		</user>
		<user>
			<workflowId>cviewer</workflowId>
			<emplId>reviewer1</emplId>
			<authenticationId>cviewer</authenticationId>
			<uuId>cviewer</uuId>
			<emailAddress>cviewer@localhost.edu</emailAddress>
			<displayName>CViewer Reviewer</displayName>
			<givenName>Viewer</givenName>
			<lastName>C</lastName>
		</user>
		<user>
			<workflowId>crover</workflowId>
			<emplId>approver1</emplId>
