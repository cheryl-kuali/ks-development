			<workgroupName>CourseSubmitters</workgroupName>
			<description>Employees who submit course propsals</description>
			<members>
				<authenticationId>csubmit</authenticationId>
			</members>
		</workgroup>
		<workgroup allowOverwrite="true">
			<workgroupName>CourseApprovers</workgroupName>
			<description>Users who can approve course validated course approvals</description>
			<members>
				<authenticationId>crover</authenticationId>
			</members>
		</workgroup>
		<workgroup allowOverwrite="true">
			<workgroupName>CourseReviewers</workgroupName>
			<description>Users who review courses</description>
			<members>
				<authenticationId>cviewer</authenticationId>
			</members>
		</workgroup>
	</workgroups>

</data>