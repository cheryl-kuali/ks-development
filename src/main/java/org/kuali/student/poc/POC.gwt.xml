<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="org.kuali.student.commons.ui.UICommons"/>
    <inherits name='org.kuali.student.ui.personidentity.PersonIdentity'/>
	

	<entry-point class='org.kuali.student.poc.client.POCEntryPoint'/>
	<!-- <extend-property name="locale" values="zh"/>  -->
    <servlet class="org.kuali.student.poc.server.admin.gwt.MessagesAdminServiceGWT" path="/MessagesAdminService"/>
    <servlet class="org.kuali.student.poc.server.login.gwt.LoginServiceImpl" path="/LoginService"/>
    <servlet path="/POC.jsp" class="org.apache.jasper.servlet.JspServlet" />
    
    <extend-property name="locale" values="en"/>
    <extend-property name="locale" values="zh"/>
    
    <stylesheet src="KualiStudent.css" />
    
</module>
