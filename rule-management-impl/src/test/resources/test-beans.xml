<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="persistList" class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<ref bean="funcBusRule1" />
	<!-- 			<ref bean="funcBusRule2" />
				<ref bean="funcBusRule3" />
				<ref bean="funcBusRule4" />
												-->
			</list>
		</property>
	</bean>

	<bean id="currentTime" class="java.util.Date" />

	<!-- 1 of CPR 101 OR 1 of FA 001 -->
	<bean id="funcBusRule1" class="org.kuali.student.rules.rulesmanagement.entity.BusinessRule">
		<property name="id"	value="11223344-1122-1122-1112-100000000001" />
		<property name="ruleId" value="1" />
		<property name="name" value="Intermediate CPR" />		
		<property name="description" value="enrollment co-requisites for Intermediate CPR 201" />	
		<property name="successMessage" value="Rule 1 Success Message" />	
		<property name="failureMessage" value="Rule 1 Failure Message" />	
		<property name="compiledId" value="111" />	
		<property name="anchor" value="CPR 201" />	
		<property name="businessRuleTypeKey" value="kuali.coursePrerequisite" />														
		<property name="metaData" ref="ruleMetaData" />
		<property name="ruleElements">
			<set>
				<bean id="ruleElement1" class="org.kuali.student.rules.rulesmanagement.entity.RuleElement">
					<property name="id"	value="11223344-1122-1122-1112-100000000002" />
					<property name="name" value="" />	
					<property name="description" value="" />	
					<property name="operation" value="PROPOSITION" />	
					<property name="ordinalPosition" value="1" />	
					<property name="businessRule" ref="funcBusRule1" />	
					<property name="ruleProposition" ref="ruleProposition1" />					
				</bean>	
				<bean id="ruleElement2" class="org.kuali.student.rules.rulesmanagement.entity.RuleElement">
					<property name="id"	value="11223344-1122-1122-1112-100000000003" />
					<property name="name" value="" />	
					<property name="description" value="" />					
					<property name="operation" value="OR" />	
					<property name="ordinalPosition" value="2" />	
					<property name="businessRule" ref="funcBusRule1" />				
				</bean>	
				<bean id="ruleElement3" class="org.kuali.student.rules.rulesmanagement.entity.RuleElement">
					<property name="id"	value="11223344-1122-1122-1112-100000000004" />
					<property name="name" value="" />	
					<property name="description" value="" />	
					<property name="operation" value="PROPOSITION" />	
					<property name="ordinalPosition" value="3" />	
					<property name="businessRule" ref="funcBusRule1" />	
					<property name="ruleProposition" ref="ruleProposition3" />					
				</bean>	
			</set>
		</property>						
	</bean>	

	<bean id="ruleMetaData"	class="org.kuali.student.rules.rulesmanagement.entity.RuleMetaData">
		<property name="createdBy" value="Kamal" />	
		<property name="createDate" ref="currentTime" />	
		<property name="updateBy" value="Wil" />
		<property name="updateDate" ref="currentTime"/>
		<property name="effectiveDateStart" ref="currentTime" />
		<property name="effectiveDateEnd" ref="currentTime" />	
		<property name="version" value="1.0" />				
		<property name="status" value="ACTIVE" />	
	</bean>	

	<bean id="ruleProposition1"	class="org.kuali.student.rules.rulesmanagement.entity.RuleProposition">
		<property name="id"	value="11223344-1122-1122-1112-100000000005" />	
		<property name="name" value="co-requisites" />	
		<property name="description" value="enumeration of required co-requisite courses" />	
		<property name="failureMessage" value="prop error message" />	
		<property name="comparisonDataType" value="NUMBER" />
		<property name="leftHandSide" ref="leftHandSide1" />	
		<property name="operator" value="EQUAL_TO" />				
		<property name="rightHandSide" ref="rightHandSide1" />	
	</bean>	

	<bean id="ruleProposition3"	class="org.kuali.student.rules.rulesmanagement.entity.RuleProposition">
		<property name="id"	value="11223344-1122-1122-1112-100000000007" />	
		<property name="name" value="co-requisites" />	
		<property name="description" value="enumeration of required co-requisite courses" />	
		<property name="failureMessage" value="prop error message" />
		<property name="comparisonDataType" value="NUMBER" />
		<property name="leftHandSide" ref="leftHandSide3" />	
		<property name="operator" value="EQUAL_TO" />				
		<property name="rightHandSide" ref="rightHandSide3" />	
	</bean>	
	
	<bean id="leftHandSide1"	class="org.kuali.student.rules.rulesmanagement.entity.LeftHandSide">
		<property name="yieldValueFunction" ref="yieldValueFunction1" />		
	</bean>
	
	<bean id="leftHandSide3"	class="org.kuali.student.rules.rulesmanagement.entity.LeftHandSide">
		<property name="yieldValueFunction" ref="yieldValueFunction3" />	
	</bean>
		
	<bean id="yieldValueFunction1"	class="org.kuali.student.rules.rulesmanagement.entity.YieldValueFunction">
		<property name="id"	value="11223344-1122-1122-1112-100000000006" />		
		<property name="yieldValueFunctionType" value="INTERSECTION" />	
	</bean>

	<bean id="yieldValueFunction3"	class="org.kuali.student.rules.rulesmanagement.entity.YieldValueFunction">
		<property name="id"	value="11223344-1122-1122-1112-100000000008" />	
		<property name="yieldValueFunctionType" value="INTERSECTION" />	
	</bean>
		
	<bean id="rightHandSide1"	class="org.kuali.student.rules.rulesmanagement.entity.RightHandSide">
		<property name="expectedValue" value="1" />		
	</bean>

	<bean id="rightHandSide3"	class="org.kuali.student.rules.rulesmanagement.entity.RightHandSide">
		<property name="expectedValue" value="1" />		
	</bean>		
	
</beans>