<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.kuali.student.poc.personidentity</groupId>
	<artifactId>personidentity-api</artifactId>
	<version>0.0.2-SNAPSHOT</version>
	<description />
	<parent>
		<groupId>org.kuali.student.poc</groupId>
		<artifactId>ks-api-pom</artifactId>
		<version>0.0.2-SNAPSHOT</version>
	</parent>

	<scm>
		<developerConnection>scm:svn:https://test.kuali.org/svn/student/poc/personidentity/personidentity-api/branches/personidentity-api-dev</developerConnection>
	</scm>
	<build>
   <plugins>
      <plugin>
         <groupId>org.apache.cxf</groupId>
         <artifactId>cxf-codegen-plugin</artifactId>
         <version>2.0.4-incubator</version>
         <dependencies>
            <dependency>
               <groupId>org.apache.cxf</groupId>
               <artifactId>
                  cxf-rt-frontend-jaxws
               </artifactId>
               <version>2.0.4-incubator</version>
            </dependency>
         </dependencies>
         <executions>
            <execution>
               <id>generate-wsdl</id>
               <phase>process-classes</phase>
               <configuration>
                  <className>
                     org.kuali.student.poc.wsdl.personidentity.person.PersonService
                  </className>
                  <targetNamespace>
                     http://student.kuali.org/wsdl/...
                  </targetNamespace>
                  <verbose>true</verbose>
               </configuration>
               <goals>
                  <goal>java2wsdl</goal>
               </goals>
            </execution>
         </executions>
      </plugin>
   </plugins>
</build>
	<repositories>
		<repository>
			<id>kuali.release</id>
			<url>http://kuali.umd.edu/archiva/repository/release</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>kuali.snapshot</id>
			<url>http://kuali.umd.edu/archiva/repository/snapshot</url>
			<releases>
				<enabled>false</enabled>
			</releases>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>kuali.release</id>
			<url>http://kuali.umd.edu/archiva/repository/release</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</pluginRepository>
		<pluginRepository>
			<id>kuali.snapshot</id>
			<url>http://kuali.umd.edu/archiva/repository/snapshot</url>
			<releases>
				<enabled>false</enabled>
			</releases>
		</pluginRepository>
	</pluginRepositories>
</project>